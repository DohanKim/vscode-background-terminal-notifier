{
	"name": "background-terminal-notifier",
	"displayName": "Background Terminal Notifier",
	"description": "Shows a notification when a terminal command completes in the background",
	"version": "1.0.0",
	"publisher": "jaredly",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"repository": {
		"type": "git",
		"url": ""
	},
